      "    assert sorted_res(res) == sorted_res((('from_start', 'to_stop', 'n', 'nth', 'nth_value'), [(10.0, 20.0, 5.0, 0, 10), (10.0, 20.0, 5.0, 1, 12.5), (10.0, 20.0, 5.0, 2, 15), (10.0, 20.0, 5.0, 3, 17.5), (10.0, 20.0, 5.0, 4, 20)]))\n"
     ]
    }
